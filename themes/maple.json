{
    "name": "Dark+ for maple",
    "include": "./dark_plus.json",
    "settings": [{
            "name": "Maple modifiers",
            "scope": [
                "storage.modifier.maple"
            ],
            "settings": {
                "foreground": "#ff80ff"
            }
        },
        {
            "name": "Maple quoted name",
            "scope": [
                "string.name.maple"
            ],
            "settings": {
                "foreground": "#ff8000"
            }
        },
        {
            "name": "Maple uneval expression",
            "scope": [
                "string.unevaled.maple"
            ],
            "settings": {
                "foreground": "#d0d00f"
            }
        },
        {
            "name": "Maple variables and constants",
            "scope": [
                "constant.language.maple",
                "variable.language.maple",
                "variable.options.maple"
            ],
            "settings": {
                "foreground": "#aaaaff"
            }
        },
        {
            "name": "Maple preprocessor",
            "scope": [
                "keyword.preprocessor.maple"
            ],
            "settings": {
                "foreground": "#ee5aa5"
            }
        },
        {
            "name": "Maple Functions",
            "scope": [
                "support.function.maple"
            ],
            "settings": {
                "foreground": "#aadddd"
            }
        }
    ]
}